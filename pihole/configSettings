# Configuration for the Debian Pihole build. Note that Pihole supports Debian, but not Alpine
APP="Debian Pihole+unbound server"
OPT_DISK=1
OPT_RAM=256
OPT_OSTYPE="debian"
OPT_OS_VERSION="11"

# pihole/.env  includes the following private parameters
OPT_CTID=
OPT_MACADDR=
PIHOLE_USER=
PIHOLE_PASSWORD=
PIHOLE_UID=
PIHOLE_GID=
PIHOLE_PWD=

